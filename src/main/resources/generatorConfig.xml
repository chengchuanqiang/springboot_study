<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
    <!--数据库驱动jar -->
    <classPathEntry location="D:/ccq工作空间/java/workspace-idea/mysql-connector-java-5.1.29.jar" />
    <context id="context1" >
        <!-- 为了防止生成的代码中有很多注释，比较难看，加入下面的配置控制 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>
        <!-- 注释控制完毕 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/jarvis?useUnicode=true&amp;characterEncoding=UTF8&amp;tinyInt1isBit=false&amp;serverTimezone=Asia/Shanghai"
                        userId="root"
                        password="123456" />
        <javaModelGenerator targetPackage="com.ccq.springbootkafka.domain" targetProject="./src/main/java" />
        <sqlMapGenerator targetPackage="mybatis" targetProject="./src/main/resources" />
        <javaClientGenerator targetPackage="com.ccq.springbootkafka.mapper" targetProject="./src/main/java" type="XMLMAPPER" />

        <table tableName="dashboard_chart" domainObjectName="dashboardChart">
            <generatedKey column="id" sqlStatement="mysql" identity="true" />
        </table>

    </context>
</generatorConfiguration>